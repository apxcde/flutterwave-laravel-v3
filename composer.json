{
    "name": "flutterwavedev/flutterwave-v3",
    "description": "A simple SDK for integrating to Flutterwave's Rave payment",
    "type": "library",
    "keywords": ["rave", "flutterwave", "payment", "transfers", "africa"],
    "homepage": "https://github.com/Flutterwave/Flutterwave-PHP-v3",
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Laravel\\Flutterwave\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^5.6.0|^7.2",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "mashape/unirest-php": "^3.0|^4.0"
   },
   "require-dev": {
        "phpunit/phpunit" : "^8.3",
        "scrutinizer/ocular": "^1.1",
        "satooshi/php-coveralls": ">0.7",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Flutterwave\\RaveServiceProvider"
            ],
            "aliases": {
                "Rave": "Laravel\\Flutterwave\\Facades\\Rave"
            }
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Flutterwave Developers",
            "email": "developers@flutterwavego.com"
        },
        {
            "name": "Kennedy Kioko",
            "email": "kmkioko@gmail.com"
        }
    ]
}
